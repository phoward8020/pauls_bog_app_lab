<div class="container">
  <h1>New Creature</h1>
 <%= form_for @creature, url: {action: "create"},
                         html: {class: "creature_form"} do |f| %>
  <%= f.text_field :name_common, placeholder:'common name', class: 'form-control' %>
  <%= f.text_field :name_latin, placeholder:'latin name', class: 'form-control' %>
  <%= f.text_field :image_url, placeholder:'image Url', class: 'form-control' %>
  <%= f.text_field :population_status, placeholder:'population status', class: 'form-control' %>
  <br><%= f.submit "Create", class: 'btn' %>
<% end %>


</div>